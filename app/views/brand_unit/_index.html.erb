<div class="autoMaxWidth body" id="banner" style="width: 1351px;">
	<img src="/assets/brand_pro.png" style="left: -284.5px; position: relative;">
</div>
<div class="wrap body">
	<div class="west">
		<div class="imgtit"><img src="/assets/brand_u_tit.png"></div>
		<div class="left_menu">
			<ul id="jzMenu">
				<%@orgs.each_with_index do |org,idx|%>
				<li><a id="band<%=org.id%>" class='
					<%=if org.proType ==1
					"cur"
					else 
					""
					end
					%>
					' href="product_type/products?protype=<%=org.proType%>" data-remote="true"><%=org.name%>专区</a></li>
				<%end%>
			</ul>
		</div>
	</div>
	
	<div class="rightGrid">
		<div class="brandlist">
			<%=render :partial=>"product_type/list"%>
		</div>
		<div class="clear"></div>
		<div class="brandbotline"></div>
		<div class="clear"></div>
		
		<div class="breakpage">
			<table align="center">
				<tbody>
					<tr><td><div class="pages" id="pag"></div></td></tr>
				</tbody>
			</table>
		</div>
	</div>
</div>

<div id="hiddenDiv"style="display:none"></div>
